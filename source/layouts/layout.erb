<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Public page of Antonin Cholet">
    <meta property="og:title" content=<%= current_page.data.title ? current_page.data.title : data.settings.site.title %>>
    <meta property="og:image" content=<%= data.settings.site.logo %>>
    <meta property="og:description" content="cholet.legal">
    <meta name="twitter:title" content=<%= current_page.data.title ? current_page.data.title : data.settings.site.title %>>
    <meta name="twitter:description" content="cholet.legal">
    <meta name="twitter:image:src" content=<%= data.settings.site.logo %>>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.10/css/all.css">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title ? current_page.data.title : data.settings.site.title %></title>
    <%= stylesheet_link_tag "site" %>
    <%= javascript_include_tag "site" %>
  </head>
  <body>
    <%= yield %>
  </body>
</html>
